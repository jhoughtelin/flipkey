{
    "name": "gueststream/flipkey",
    "description": "Flipkey data API wrapper.",
    "keywords": [
        "flipkey",
        "reviews",
        "api"
    ],
    "homepage": "https://github.com/jhoughtelin/flipkey",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Houghtelin",
            "email": "josh@findsomehelp.com",
            "homepage": "http://www.gitphp.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.4.0",
        "guzzle/guzzle" : "~3.7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "FlipKey\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FlipKey\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
